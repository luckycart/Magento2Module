{
    "name": "yuukooagency/luckycart",
    "description": "Lucky Cart implementation on Magento 2",
    "type": "magento2-module",
    "license": "GPL-3.0",
    "authors": [
        {
            "email": "support@yuukoo.com",
            "name": "Yuukoo"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "luckycart/luckycart": "^2.8"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Yuukoo\\Luckycart\\": ""
        }
    },
    "repositories": [
    	{
			"type": "package",
			"package" : {
				"name": "luckycart/luckycart",
				"version": "2.8",
				"dist": {
					"url": "https://s3-eu-west-1.amazonaws.com/download.luckycart.com/integration/php/luckycart.php",
					"type": "file"
				}
			}
    	}
    ]
}
